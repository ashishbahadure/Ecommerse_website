<%- include ('nav.ejs') %>
    <div class="container-fluid bg-white p-3">
        <div class="row">
            <div class="col-12">
                <h3>Product List</h3>
                <br>
                <table class="table table-bordered table-striped">
                    <tr>
                        <th class="text-dark">SN</th>
                        <th class="text-dark">Category</th>
                        <th class="text-dark">Subcategory</th>
                        <th class="text-dark">Product</th>
                        <th class="text-dark">Price</th>
                        <th class="text-dark">Company</th>
                        <th class="text-dark">Rating</th>
                    </tr>
                    <% for(i=0;i<products.length;i++){ %>
                        <tr>
                            <td class="text-dark"><%= i+1 %></td>
                            <td class="text-dark"><%=products[i].category_name%></td>
                            <td class="text-dark"><%=products[i].sub_cat_name%></td>
                            <td class="text-dark"><%=products[i].product_name%></td>
                            <td class="text-dark">
                                <del>
                                    &#8377;<%=products[i].duplicate_price%></del>
                                    &#8377; <%=products[i].product_price%>
                            </td>
                            <td class="text-dark"><%=products[i].product_company%></td>
                            <td class="text-dark"><%=products[i].product_rating%></td>
                        </tr>
                    <% } %>
                </table>
            </div>
        </div>
    </div>
<%- include ('footer.ejs') %>